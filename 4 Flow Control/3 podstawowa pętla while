using System;
					
public class Program
{
	public static void Main()
	{
		int liczba;
		
		do
		{

			Console.WriteLine("Podaj Liczbę ");
			liczba = int.Parse(Console.ReadLine());
			if (liczba == 0)
				break;
			Console.WriteLine(liczba * 10);
				
		}
		while (liczba != 0);	
			
		
		// powinno się poprostu użyć while !=0 
	}
}
