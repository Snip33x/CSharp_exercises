using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace cokolwiek
{
	public class Program
	{
		public static void Main()
		{

			for (int j = 2; j<= 6; j++)
			{


				for (int i = 1; i <= 10; i++)
				{
					Console.WriteLine(j + " x " + i + " = " + i * j);
				}
				Console.WriteLine("\n");
			}
			Console.ReadKey();
		}
	}
}
