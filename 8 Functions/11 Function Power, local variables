using System;
					
public class Program
{
	
	public static int Power(int liczba1,int liczba2)
	{
	
		int result = 1;
		for (int i = 0; i < liczba2; i++)
			result *= liczba1;
		
			return result;
	}
	
	public static void Main()
	{
		
		int x = 3;
		int y = 3;
	

		Console.WriteLine("x:{0} ^ y:{1} równa się {2}", x,y, Power(x,y));
	}
}


